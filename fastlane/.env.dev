# fastlane/.env.dev
# Environment variables for Development builds

# App Configuration
APP_NAME=MyApp Dev
PACKAGE_NAME=com.example.myapp.dev
VERSION_NAME=1.0.0

# Firebase Configuration
FIREBASE_APP_ID=1:123456789:android:abcdef123456
FIREBASE_TOKEN=your_firebase_token_here

# Keystore Configuration (Loaded from Jenkins Credentials)
KEYSTORE_FILE=/path/to/keystore.jks
KEYSTORE_PASSWORD=your_keystore_password
KEY_ALIAS=your_key_alias
KEY_PASSWORD=your_key_password

# SonarQube Configuration
SONAR_HOST_URL=http://your-sonarqube-server:9000
SONAR_TOKEN=your_sonar_token
SONAR_PROJECT_KEY=com.example.myapp.dev

# Git Information (Populated by Jenkins)
#GIT_BRANCH=${GIT_BRANCH}
#GIT_COMMIT=${GIT_COMMIT}